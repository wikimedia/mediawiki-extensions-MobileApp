{
	"name": "MobileApp",
	"author": [
		"Yuvi Panda"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:MobileApp",
	"descriptionmsg": "mobileapp-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.45"
	},
	"MessagesDirs": {
		"MobileApp": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\MobileApp\\": "includes/"
	},
	"Hooks": {
		"ListDefinedTags": "main",
		"ChangeTagsListActive": "main",
		"RecentChange_save": "main",
		"AbuseFilter-generateUserVars": "abusefilter",
		"AbuseFilter-builder": "abusefilter"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\MobileApp\\Hooks"
		},
		"abusefilter": {
			"class": "MediaWiki\\Extension\\MobileApp\\AbuseFilterHooks",
			"services": [
				"ConnectionProvider",
				"ChangeTagsStore"
			]
		}
	},
	"manifest_version": 2
}
